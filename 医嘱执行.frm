Private Sub Command1_Click()
On Error Resume Next
Adodc1.Recordset.MoveFirst
cc = Adodc1.Recordset.RecordCount

For i = 0 To cc Step 1

If Adodc1.Recordset.Fields("医嘱日期") = Date Then
Adodc1.Recordset.Fields("状态") = "完成"
Else
Adodc1.Recordset.Fields("医嘱日期") = DateAdd("d", 1, Adodc1.Recordset.Fields("医嘱日期"))
End If
Adodc2.Recordset.AddNew
Adodc2.Recordset.Fields("患者姓名") = Adodc1.Recordset.Fields("病人姓名")
Adodc2.Recordset.Fields("床号") = Adodc1.Recordset.Fields("床号")
Adodc2.Recordset.Fields("住院号") = Adodc1.Recordset.Fields("住院号")
Adodc2.Recordset.Fields("医嘱名称") = Adodc1.Recordset.Fields("医嘱名称")
Adodc2.Recordset.Fields("规格") = Adodc1.Recordset.Fields("规格")
Adodc2.Recordset.Fields("数量") = Adodc1.Recordset.Fields("一次数量")
Adodc2.Recordset.Fields("医生") = Adodc1.Recordset.Fields("医生")
Adodc2.Recordset.Fields("护士") = Label2.Caption
Adodc2.Recordset.Fields("日期") = Date
Adodc2.Recordset.Fields("时间") = Time
Adodc2.Recordset.Update

Adodc1.Recordset.MoveNext
If Adodc1.Recordset.EOF = True Then
Adodc1.Recordset.Update
Exit Sub
End If
Next i
End Sub

Private Sub Command2_Click()
Text1.Text = Val(Text1.Text) - 1
End Sub

Private Sub Command3_Click()
